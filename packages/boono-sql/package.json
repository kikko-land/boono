{
  "name": "@kikko-land/boono-sql",
  "version": "0.3.0",
  "author": "Sergey Popov",
  "license": "MIT",
  "homepage": "https://boono.netlify.app",
  "description": "An advanced SQL builder, specially tailored for SQLite",
  "keywords": [
    "typescript",
    "sql",
    "sqlite",
    "typesafe",
    "sqlite3",
    "sql-builder"
  ],
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "es2015": "./dist/index.es.js",
        "require": "./dist/index.umd.js",
        "import": "./dist/index.es.js",
        "default": "./dist/index.es.js"
      }
    },
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "yarn vite build",
    "lint": "eslint \"src/**/*.{tsx,ts}\" --max-warnings=0",
    "check-typing": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {},
  "devDependencies": {
    "@kikko-land/common-scripts": "^0.1.1",
    "vitest": "^0.27.0"
  }
}